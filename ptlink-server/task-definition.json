{
    "family": "PtlServerTaskDef",
    "containerDefinitions": [
        {
            "name": "PtlServerContainer",
            "image": "203918852273.dkr.ecr.ap-northeast-2.amazonaws.com/ptlink/ptlink-server:latest",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "port8080",
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/PtlServerTaskDef",
                    "mode": "non-blocking",
                    "awslogs-create-group": "true",
                    "max-buffer-size": "25m",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "systemControls": []
        }
    ],
    "taskRoleArn": "arn:aws:iam::203918852273:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::203918852273:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "949",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}